<?xml version="1.0" ?>
<launch>

    <arg name="veh" default="duckie_alpha" />

    <group ns="$(arg veh)">

        <node pkg="joy" type="joy_node" name="joy_node">
            <param name="autorepeat_rate" value="20" />
        </node>

        <node pkg="rl_navigation" type="teleop.py" name="teleop" />

    </group>

</launch>